    "N, M = map(int, input().split())\n",
    "result_list = [0 for _ in range(N)]\n",
    "\n",
    "for _ in range(M):\n",
    "    i, j, k = map(int, input().split())\n",
    "    for a in range(i-1, j):\n",
    "        result_list[a] = k\n",
    "for i in range(N):\n",
    "    print(result_list[i], end=\" \")"
